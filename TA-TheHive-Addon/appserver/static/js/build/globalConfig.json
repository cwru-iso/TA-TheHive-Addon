{
    "meta": {
        "displayName": "TheHive Add-on", 
        "restRoot": "TA_TheHive_Addon", 
        "name": "TA-TheHive-Addon", 
        "version": "1.0.0", 
        "apiVersion": "3.0.0"
    }, 
    "pages": {
        "configuration": {
            "title": "Configuration", 
            "description": "Set up your add-on", 
            "tabs": [
                {
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }, 
                    "title": "Proxy", 
                    "name": "proxy", 
                    "entity": [
                        {
                            "type": "checkbox", 
                            "field": "proxy_enabled", 
                            "label": "Enable"
                        }, 
                        {
                            "options": {
                                "disableSearch": true, 
                                "autoCompleteFields": [
                                    {
                                        "label": "http", 
                                        "value": "http"
                                    }, 
                                    {
                                        "label": "socks4", 
                                        "value": "socks4"
                                    }, 
                                    {
                                        "label": "socks5", 
                                        "value": "socks5"
                                    }
                                ]
                            }, 
                            "type": "singleSelect", 
                            "defaultValue": "http", 
                            "field": "proxy_type", 
                            "label": "Proxy Type"
                        }, 
                        {
                            "type": "text", 
                            "validators": [
                                {
                                    "type": "string", 
                                    "maxLength": 4096, 
                                    "minLength": 0, 
                                    "errorMsg": "Max host length is 4096"
                                }
                            ], 
                            "field": "proxy_url", 
                            "label": "Host"
                        }, 
                        {
                            "type": "text", 
                            "validators": [
                                {
                                    "type": "number", 
                                    "range": [
                                        1, 
                                        65535
                                    ]
                                }
                            ], 
                            "field": "proxy_port", 
                            "label": "Port"
                        }, 
                        {
                            "type": "text", 
                            "validators": [
                                {
                                    "type": "string", 
                                    "maxLength": 50, 
                                    "minLength": 0, 
                                    "errorMsg": "Max length of username is 50"
                                }
                            ], 
                            "field": "proxy_username", 
                            "label": "Username"
                        }, 
                        {
                            "type": "text", 
                            "encrypted": true, 
                            "field": "proxy_password", 
                            "validators": [
                                {
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192"
                                }
                            ], 
                            "label": "Password"
                        }, 
                        {
                            "type": "checkbox", 
                            "field": "proxy_rdns", 
                            "label": "Remote DNS resolution"
                        }
                    ]
                }, 
                {
                    "title": "Logging", 
                    "name": "logging", 
                    "entity": [
                        {
                            "options": {
                                "disableSearch": true, 
                                "autoCompleteFields": [
                                    {
                                        "label": "DEBUG", 
                                        "value": "DEBUG"
                                    }, 
                                    {
                                        "label": "INFO", 
                                        "value": "INFO"
                                    }, 
                                    {
                                        "label": "WARNING", 
                                        "value": "WARNING"
                                    }, 
                                    {
                                        "label": "ERROR", 
                                        "value": "ERROR"
                                    }, 
                                    {
                                        "label": "CRITICAL", 
                                        "value": "CRITICAL"
                                    }
                                ]
                            }, 
                            "type": "singleSelect", 
                            "defaultValue": "INFO", 
                            "field": "loglevel", 
                            "label": "Log level"
                        }
                    ]
                }, 
                {
                    "title": "Add-on Settings", 
                    "name": "additional_parameters", 
                    "entity": [
                        {
                            "help": "Base URL for your instance of TheHive. eg: \"https://thehive.example.com\"", 
                            "type": "text", 
                            "required": true, 
                            "defaultValue": "", 
                            "field": "thehive_url", 
                            "validators": [
                                {
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0, 
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ], 
                            "label": "TheHive URL"
                        }, 
                        {
                            "help": "API key for the user account that will be creating alerts in TheHive.", 
                            "type": "text", 
                            "required": true, 
                            "defaultValue": "", 
                            "field": "thehive_key", 
                            "encrypted": true, 
                            "validators": [
                                {
                                    "type": "string", 
                                    "maxLength": 8192, 
                                    "minLength": 0, 
                                    "errorMsg": "Max length of password is 8192"
                                }
                            ], 
                            "label": "API Key"
                        }
                    ]
                }
            ]
        }
    }
}